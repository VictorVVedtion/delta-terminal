# AI Orchestrator Service 配置

# 服务配置
PORT=4010
HOST=0.0.0.0
NODE_ENV=development

# OpenRouter API
OPENROUTER_API_KEY=your-openrouter-api-key
OPENROUTER_API_URL=https://openrouter.ai/api/v1

# Redis 缓存
REDIS_URL=redis://localhost:6379

# Python NLP Processor
NLP_PROCESSOR_URL=http://localhost:8001

# Python Strategy Generator
STRATEGY_GENERATOR_URL=http://localhost:8002

# 日志级别
LOG_LEVEL=info

# 速率限制
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=60000

# JWT 密钥 (与 auth-service 共享)
JWT_SECRET=your-jwt-secret

# 成本控制
DEFAULT_MONTHLY_CREDIT_LIMIT=100
MAX_TOKENS_PER_REQUEST=4096
